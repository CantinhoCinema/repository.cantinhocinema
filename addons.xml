<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="plugin.program.cantinhocs" name="cantinhocs Wizard" version="1.1" provider-name="cantinhocs">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">cantinhocs Wizard</summary>
    <description lang="en">cantinhocs Wizard allows you to get access to the latest Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="plugin.video.iptvCS" name="IPTV CS" version="0.1" provider-name="IPTV CS">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
    <import addon="script.module.beautifulsoup" version="3.2.1"/>
    <import addon="script.module.simple.downloader" version="0.9.4"/>
    <import addon="script.module.beautifulsoup4"/>
    <import addon="script.module.requests"/>
    <import addon="script.module.httplib2"/>
    <import addon="script.module.youtube.dl" optional="true"/>
    <import addon="plugin.video.youtube"/>
    <import addon="script.module.urlresolver" optional="true"/>
    <import addon="script.module.simplejson"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en"></summary>
    <description lang="en">Mais um addon PT</description>
    <platform>all</platform>
 </extension>
</addon>

